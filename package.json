{
  "name": "express-rest-api-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./src/bin/www",
    "dev": "NODE_ENV=development nodemon ./src/bin/www",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix --ext .js",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.12",
    "prettier": "^1.13.0",
    "supertest": "^3.4.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "ignore": [
      "*.test.js"
    ]
  }
}
